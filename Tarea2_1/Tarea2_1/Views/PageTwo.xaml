<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Tarea2_1.Views.PageTwo">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding nombre}"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" x:Name="lblresultado"/>

            <Label Text="{Binding apellidos}"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" x:Name="lblresultado2"/>

            <Label Text="{Binding edad}"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" x:Name="lblresultado3"/>
            
            <Label Text="{Binding correo}"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" x:Name="lblresultado4"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>